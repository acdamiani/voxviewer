{
  "name": "voxviewer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:prod": "./build_prod.sh",
    "build:wasm": "./build_wasm.sh",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore \"./src/**/*.+(ts|js|svelte)\"",
    "lint-fix": "eslint --ignore-path .gitignore --fix \"./src/**/*.+(ts|js|svelte)\"",
    "format": "prettier --ignore-path .gitignore \"./src/**/*.+(ts|js|svelte|html)\" --write"
  },
  "devDependencies": {
    "@iconify-json/fa6-brands": "1.1.11",
    "@iconify-json/fa6-solid": "1.1.13",
    "@iconify-json/octicon": "1.1.37",
    "@sveltejs/adapter-vercel": "2.4.1",
    "@sveltejs/kit": "1.15.5",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte3": "4.0.0",
    "postcss": "8.4.21",
    "prettier": "2.8.7",
    "prettier-plugin-svelte": "2.10.0",
    "svelte": "3.58.0",
    "svelte-check": "3.2.0",
    "tailwind-scrollbar-hide": "1.1.7",
    "tailwindcss": "3.3.1",
    "tslib": "2.5.0",
    "typescript": "5.0.4",
    "unplugin-icons": "0.16.1",
    "vite": "4.2.1",
    "vite-plugin-wasm-pack": "0.1.12"
  },
  "type": "module",
  "dependencies": {
    "decimal.js": "10.4.3",
    "shortid": "2.2.16",
    "waveform-data": "4.3.0"
  }
}
