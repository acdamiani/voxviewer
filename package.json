{
  "name": "voxviewer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:prod": "./build_prod.sh",
    "build:wasm": "./build_wasm.sh",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore \"./src/**/*.+(ts|js|svelte)\"",
    "lint-fix": "eslint --ignore-path .gitignore --fix \"./src/**/*.+(ts|js|svelte)\"",
    "format": "prettier --ignore-path .gitignore \"./src/**/*.+(ts|js|svelte|html)\" --write"
  },
  "devDependencies": {
    "@iconify-json/fa6-brands": "1.2.6",
    "@iconify-json/fa6-solid": "1.2.4",
    "@iconify-json/octicon": "1.2.10",
    "@sveltejs/adapter-vercel": "5.10.2",
    "@sveltejs/kit": "1.30.4",
    "@types/shortid": "0.0.32",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "autoprefixer": "10.4.21",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.2",
    "eslint-plugin-svelte3": "4.0.0",
    "postcss": "8.5.6",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "2.10.1",
    "svelte": "4.2.20",
    "svelte-check": "3.8.6",
    "tailwind-scrollbar-hide": "1.3.1",
    "tailwindcss": "3.4.17",
    "tslib": "2.8.1",
    "typescript": "5.7.3",
    "unplugin-icons": "0.22.0",
    "vite": "4.5.14",
    "vite-plugin-wasm-pack": "0.1.12"
  },
  "type": "module",
  "dependencies": {
    "decimal.js": "10.4.3",
    "shortid": "2.2.16",
    "waveform-data": "4.5.2"
  }
}
