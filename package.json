{
  "name": "voxviewer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:prod": "./build_prod.sh",
    "build:wasm": "./build_wasm.sh",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore \"./src/**/*.+(ts|js|svelte)\"",
    "lint-fix": "eslint --ignore-path .gitignore --fix \"./src/**/*.+(ts|js|svelte)\"",
    "format": "prettier --ignore-path .gitignore \"./src/**/*.+(ts|js|svelte|html)\" --write"
  },
  "devDependencies": {
    "@iconify-json/fa6-brands": "1.1.11",
    "@iconify-json/fa6-solid": "1.1.13",
    "@iconify-json/octicon": "1.1.44",
    "@sveltejs/adapter-vercel": "2.4.3",
    "@sveltejs/kit": "1.22.1",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte3": "4.0.0",
    "postcss": "8.4.25",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "2.10.1",
    "svelte": "3.59.2",
    "svelte-check": "3.4.5",
    "tailwind-scrollbar-hide": "1.1.7",
    "tailwindcss": "3.3.2",
    "tslib": "2.6.0",
    "typescript": "5.1.6",
    "unplugin-icons": "0.16.3",
    "vite": "4.4.2",
    "vite-plugin-wasm-pack": "0.1.12"
  },
  "type": "module",
  "dependencies": {
    "decimal.js": "10.4.3",
    "shortid": "2.2.16",
    "waveform-data": "4.3.0"
  }
